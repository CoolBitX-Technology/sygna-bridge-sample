{
    "title": "API_INPUT",
    "description": "describes properties required to check",
    "type": "object",
    "properties": {
        "data":{
            "type":"object",
            "properties":{
                "private_info":{
                    "type":"string"
                },
                "transaction": {
                    "type": "object",
                    "properties": {
                        "originator_vasp_code":{
                            "type": "string"
                        },
                        "originator_addr":{
                            "type": "string"
                        },
                        "beneficiary_vasp_code":{
                            "type": "string"
                        },
                        "beneficiary_addr":{
                            "type": "string"
                        },
                        "transaction_currency":{
                            "type": "string"
                        },
                        "amount":{
                            "type": "number"
                        }
                    },
                    "required":[ "originator_vasp_code", "originator_addr", "beneficiary_vasp_code", "beneficiary_addr", "transaction_currency", "amount" ]
                },
                "signature": {
                    "type": "string",
                    "maxLength":128,
                    "minLength":128
                },
                "data_dt": {
                    "type": "string"
                }
            },
            "required": [
                "private_info",
                "transaction",
                "signature",
                "data_dt"
            ]
        },
        "callback":{
            "type":"object",
            "properties":{
                "callback_url" : {
                    "type": "string"
                },
                "signature": {
                    "type": "string",
                    "maxLength":128,
                    "minLength":128
                }
            },
            "required": ["callback_url", "signature"]
        },
        "transfer_id": {
            "type": "string"
        }
    },
    "required": [ "data", "callback", "transfer_id" ],
    "validateStrength": true
}
